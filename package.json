{
  "name": "dataset-api",
  "version": "0.0.1",
  "description": "",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.test.json && cross-env TS_NODE_FILES=true tap test/**/*.test.ts",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "dev": "tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"tsc -w\" \"fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kobra-dev/dataset-api.git"
  },
  "keywords": [],
  "author": "Makuza Mugabo Verite <mugaboverite@gmail.com> (https://veritem.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kobra-dev/dataset-api/issues"
  },
  "homepage": "https://github.com/kobra-dev/dataset-api#readme",
  "dependencies": {
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.3.1",
    "fastify-cli": "^2.11.0",
    "fastify-env": "^2.1.1",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^15.0.0",
    "@types/tap": "^15.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fastify-tsconfig": "^1.0.1",
    "prettier": "^2.2.1",
    "tap": "^14.11.0",
    "typescript": "^4.1.3"
  }
}
